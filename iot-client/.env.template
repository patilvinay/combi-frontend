# Event Hub Connection String for IoT Hub's built-in endpoint
EVENTHUB_CONNECTION_STRING=Endpoint=sb://your-iothub.servicebus.windows.net/;SharedAccessKeyName=service;SharedAccessKey=your-key;EntityPath=your-iothub

# Event Hub Consumer Group (default: $Default)
CONSUMER_GROUP=$Default

# Device ID to monitor
DEVICE_ID=your-device-id
